{
	"name": "hoganslender-tanium",
	"displayName": "Tanium Tools",
	"description": "",
	"version": "0.0.4",
	"publisher": "HoganSlender",
	"repository": {
		"type": "git",
		"url": "https://github.com/HoganSlender/vscode-tanium-tools.git"
	},
	"bugs": {
		"url": "https://github.com/HoganSlender/vscode-tanium-tools/issues",
		"email": "hoganslender@gmail.com"
	},
	"engines": {
		"vscode": "^1.46.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:hoganslendertanium.compareContentSetSensors",
		"onCommand:hoganslendertanium.compareServerServerSensors",
		"onCommand:hoganslendertanium.compareServerServerContentSets",
		"onCommand:hoganslendertanium.compareServerServerContentSetPrivileges",
		"onCommand:hoganslendertanium.compareServerServerContentSetRoles",
		"onCommand:hoganslendertanium.compareServerServerContentSetRolePrivileges",
		"onCommand:hoganslendertanium.compareServerServerContentSetRoleMemberships",
		"onCommand:hoganslendertanium.compareServerServerUsers",
		"onCommand:hoganslendertanium.generateExportFileMissingSensors",
		"onCommand:hoganslendertanium.signContentFile",
		"onCommand:hoganslendertanium.generateExportFileModifiedSensors",
		"onCommand:hoganslendertanium.retrieveSensorByName",
		"onCommand:hoganslendertanium.retrieveSensorByHash",
		"onCommand:hoganslendertanium.compareServerServerPackages",
		"onCommand:hoganslendertanium.analyzePackages",
		"onCommand:hoganslendertanium.analyzeContentSets",
		"onCommand:hoganslendertanium.analyzeContentSetPrivileges",
		"onCommand:hoganslendertanium.analyzeContentSetRoles",
		"onCommand:hoganslendertanium.analyzeContentSetRolePrivileges",
		"onCommand:hoganslendertanium.analyzeUsers",
		"onCommand:hoganslendertanium.analyzeContentSetRoleMemberships",
		"onCommand:hoganslendertanium.downloadPackageFiles",
		"onCommand:hoganslendertanium.workspaceInit"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "hoganslendertanium.compareContentSetSensors",
				"title": "Compare Content Set Sensors",
				"category": "Tanium"
			},
			{
				"command": "hoganslendertanium.compareServerServerSensors",
				"title": "Compare Tanium Server Sensors",
				"category": "Tanium"
			},
			{
				"command": "hoganslendertanium.compareServerServerContentSets",
				"title": "Compare Tanium Server Content Sets",
				"category": "Tanium"
			},
			{
				"command": "hoganslendertanium.compareServerServerContentSetPrivileges",
				"title": "Compare Tanium Server Content Set Privileges",
				"category": "Tanium"
			},
			{
				"command": "hoganslendertanium.compareServerServerContentSetRoles",
				"title": "Compare Tanium Server Content Set Roles",
				"category": "Tanium"
			},
			{
				"command": "hoganslendertanium.compareServerServerContentSetRolePrivileges",
				"title": "Compare Tanium Server Content Set Role Privileges",
				"category": "Tanium"
			},
			{
				"command": "hoganslendertanium.compareServerServerContentSetRoleMemberships",
				"title": "Compare Tanium Server Content Set Role Memberships",
				"category": "Tanium"
			},
			{
				"command": "hoganslendertanium.compareServerServerUsers",
				"title": "Compare Tanium Server Users",
				"category": "Tanium"
			},
			{
				"command": "hoganslendertanium.generateExportFileMissingSensors",
				"title": "Generate Export File for Missing Sensors",
				"category": "Tanium"
			},
			{
				"command": "hoganslendertanium.signContentFile",
				"title": "Sign Content File",
				"category": "Tanium"
			},
			{
				"command": "hoganslendertanium.generateExportFileModifiedSensors",
				"title": "Generate Export File for Modified Sensors",
				"category": "Tanium"
			},
			{
				"command": "hoganslendertanium.retrieveSensorByName",
				"title": "Retrieve Sensor Data by Name",
				"category": "Tanium"
			},
			{
				"command": "hoganslendertanium.retrieveSensorByHash",
				"title": "Retrieve Sensor Data by Hash",
				"category": "Tanium"
			},
			{
				"command": "hoganslendertanium.compareServerServerPackages",
				"title": "Compare Tanium Server Packages",
				"category": "Tanium"
			},
			{
				"command": "hoganslendertanium.analyzePackages",
				"title": "Analyze Packages",
				"category": "Tanium"
			},
			{
				"command": "hoganslendertanium.analyzeContentSets",
				"title": "Analyze Content Sets",
				"category": "Tanium"
			},
			{
				"command": "hoganslendertanium.analyzeContentSetPrivileges",
				"title": "Analyze Content Set Privileges",
				"category": "Tanium"
			},
			{
				"command": "hoganslendertanium.analyzeContentSetRoles",
				"title": "Analyze Content Set Roles",
				"category": "Tanium"
			},
			{
				"command": "hoganslendertanium.analyzeContentSetRolePrivileges",
				"title": "Analyze Content Set Role Privileges",
				"category": "Tanium"
			},
			{
				"command": "hoganslendertanium.analyzeUsers",
				"title": "Analyze Users",
				"category": "Tanium"
			},
			{
				"command": "hoganslendertanium.analyzeContentSetRoleMemberships",
				"title": "Analyze Content Set Role Memberships",
				"category": "Tanium"
			},
			{
				"command": "hoganslendertanium.downloadPackageFiles",
				"title": "Download Package Files",
				"category": "Tanium"
			},
			{
				"command": "hoganslendertanium.workspaceInit",
				"title": "Setup Tanium Workspace",
				"category": "Tanium"
			}
		],
		"menus": {
			"commandPalette": [
				{
					"command": "hoganslendertanium.generateExportFileMissingSensors",
					"when": "false"
				},
				{
					"command": "hoganslendertanium.signContentFile",
					"when": "false"
				},
				{
					"command": "hoganslendertanium.generateExportFileModifiedSensors",
					"when": "false"
				},
				{
					"command": "hoganslendertanium.analyzePackages",
					"when": "false"
				},
				{
					"command": "hoganslendertanium.analyzeContentSets",
					"when": "false"
				},
				{
					"command": "hoganslendertanium.analyzeContentSetPrivileges",
					"when": "false"
				},
				{
					"command": "hoganslendertanium.analyzeContentSetRoles",
					"when": "false"
				},
				{
					"command": "hoganslendertanium.analyzeContentSetRolePrivileges",
					"when": "false"
				},
				{
					"command": "hoganslendertanium.analyzeUsers",
					"when": "false"
				},
				{
					"command": "hoganslendertanium.analyzeContentSetRoleMemberships",
					"when": "false"
				}
			],
			"explorer/context": [
				{
					"command": "hoganslendertanium.generateExportFileMissingSensors",
					"group": "3_compare@1",
					"when": "explorerResourceIsFolder && listDoubleSelection"
				},
				{
					"command": "hoganslendertanium.signContentFile",
					"group": "3_compare@1",
					"when": "!explorerResourceIsFolder"
				},
				{
					"command": "hoganslendertanium.generateExportFileModifiedSensors",
					"group": "3_compare@1",
					"when": "explorerResourceIsFolder && listDoubleSelection"
				},
				{
					"command": "hoganslendertanium.analyzePackages",
					"group": "3_compare@1",
					"when": "explorerResourceIsFolder && listDoubleSelection&& resourceFilename =~ /.*%Packages$/"
				},
				{
					"command": "hoganslendertanium.analyzeContentSets",
					"group": "3_compare@1",
					"when": "explorerResourceIsFolder && listDoubleSelection&& resourceFilename =~ /.*%ContentSets$/"
				},
				{
					"command": "hoganslendertanium.analyzeContentSetPrivileges",
					"group": "3_compare@1",
					"when": "explorerResourceIsFolder && listDoubleSelection&& resourceFilename =~ /.*%ContentSetPrivileges$/"
				},
				{
					"command": "hoganslendertanium.analyzeContentSetRoles",
					"group": "3_compare@1",
					"when": "explorerResourceIsFolder && listDoubleSelection&& resourceFilename =~ /.*%ContentSetRoles$/"
				},
				{
					"command": "hoganslendertanium.analyzeContentSetRolePrivileges",
					"group": "3_compare@1",
					"when": "explorerResourceIsFolder && listDoubleSelection && resourceFilename =~ /.*%ContentSetRolePrivileges$/"
				},
				{
					"command": "hoganslendertanium.analyzeUsers",
					"group": "3_compare@1",
					"when": "explorerResourceIsFolder && listDoubleSelection && resourceFilename =~ /.*%Users$/"
				},
				{
					"command": "hoganslendertanium.analyzeContentSetRoleMemberships",
					"group": "3_compare@1",
					"when": "explorerResourceIsFolder && listDoubleSelection && resourceFilename =~ /.*%ContentSetRoleMemberships$/"
				}
			]
		},
		"configuration": {
			"type": "object",
			"title": "Tanium Tools",
			"properties": {
				"hoganslender.tanium.allowSelfSignedCerts": {
					"type": "boolean",
					"default": false,
					"description": "Set to true if communicating with servers that are using a self signed certificate."
				},
				"hoganslender.tanium.fqdns": {
					"type": "array",
					"default": [],
					"description": "List of recently used Tanium server FQDNs."
				},
				"hoganslender.tanium.httpTimeoutSeconds": {
					"type": "number",
					"default": 10,
					"description": "Number of seconds for HTTP request timeout."
				},
				"hoganslender.tanium.signingPaths": {
					"type": "array",
					"description": "Path definitions for KeyUtility.exe and private key file"
				},
				"hoganslender.tanium.usernames": {
					"type": "array",
					"default": [],
					"description": "List of recently used Tanium usernames."
				}
			}
		}
	},
	"scripts": {
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js",
		"vscode:prepublish": "webpack --mode production",
		"webpack": "webpack --mode development",
		"webpack-dev": "webpack --mode development --watch",
		"test-compile": "tsc -p ./"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^14.0.27",
		"@types/vscode": "^1.46.0",
		"@typescript-eslint/eslint-plugin": "^3.8.0",
		"@typescript-eslint/parser": "^3.8.0",
		"eslint": "^7.6.0",
		"glob": "^7.1.6",
		"mocha": "^8.0.1",
		"ts-loader": "^8.0.3",
		"typescript": "^3.8.3",
		"vscode-test": "^1.4.0",
		"webpack": "^4.44.1",
		"webpack-cli": "^3.3.12"
	},
	"dependencies": {
		"@types/diff-match-patch": "^1.0.32",
		"@types/got": "^9.6.11",
		"@types/he": "^1.1.1",
		"@types/lodash": "^4.14.161",
		"@types/pug": "^2.0.4",
		"@types/uuid": "^8.3.0",
		"diff-match-patch": "^1.0.5",
		"fast-xml-parser": "^3.17.4",
		"got": "^11.6.0",
		"he": "^1.2.0",
		"lodash": "^4.17.20",
		"pug": "^3.0.0",
		"sanitize-filename-ts": "^1.0.2",
		"uuid": "^8.3.0"
	}
}
